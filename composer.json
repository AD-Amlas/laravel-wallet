{
  "name": "bavix/laravel-wallet",
  "description": "Easy work with virtual wallet.",
  "keywords": [
    "laravel",
    "laravel5",
    "credits",
    "bavix",
    "currency",
    "laravel5-package",
    "laravel-wallet",
    "virtual",
    "wallet",
    "payments"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/bavix/laravel-wallet",
  "license": "MIT",
  "authors": [
    {
      "name": "Babichev Maxim",
      "email": "info@babichev.net"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "illuminate/database": "^5.5|^6.0",
    "nesbot/carbon": "^1.20|^2.0",
    "doctrine/dbal": "^2.8",
    "ramsey/uuid": "^3.0"
  },
  "require-dev": {
    "infection/infection": "^0.13",
    "orchestra/testbench": "^3.7|^4.0",
    "phpstan/phpstan": "^0.11",
    "phpunit/phpunit": "^7.5|^8.2"
  },
  "suggest": {
    "bavix/laravel-wallet-swap": "Addition to the laravel-wallet library for quick setting of exchange rates"
  },
  "autoload": {
    "psr-4": {
      "Bavix\\Wallet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bavix\\Wallet\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bavix\\Wallet\\WalletServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
